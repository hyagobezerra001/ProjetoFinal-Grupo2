<page layout="3columns" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
        <referenceBlock name="logo">
        <arguments>
            <argument name="logo_width" xsi:type="number">221</argument>
            <argument name="logo_height" xsi:type="number">40</argument>
            <argument name="logo_file" xsi:type="string">images/logo_copy.png</argument>
        </arguments>
    </referenceBlock>

    <referenceContainer name="footer-container">
        <container name="footer-links" htmlTag="div" htmlClass="footer-links">
            <block class="Magento\Cms\Block\Block" name="link1" before="-">
                <arguments>
                    <argument name="block_id" xsi:type="string">footer-links1_Fashion</argument>
                </arguments>
            </block>
            <block class="Magento\Cms\Block\Block" name="link2" before="-">
                <arguments>
                    <argument name="block_id" xsi:type="string">footer-links1_Fashion_en</argument>
                </arguments>
            </block>
            <block class="Magento\Newsletter\Block\Subscribe" name="link3" after="-">
                <arguments>
                    <argument name="block_id" xsi:type="string">form.subscribe</argument>
                </arguments>
            </block>
        </container>
    </referenceContainer>
    <!-- <move element="form.subscribe" destination="footer-container" after="-" /> -->
    <referenceBlock name="footer_links" remove="true"></referenceBlock>
    <referenceBlock name="ui-state-focus" remove="true"></referenceBlock>
    <referenceBlock name="all-category" remove="true" />



</page>
